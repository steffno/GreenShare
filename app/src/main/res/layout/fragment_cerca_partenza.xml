<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/cercaPartenzaFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#002233"
    android:gravity="top"
    android:theme="@style/ThemeOverlay.GreenShare.FullscreenContainer"
    tools:context=".ui.viaggio.CercaPartenzaFragment">

    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
        android:id="@+id/cercaPartenzaAutocomplete"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="4dp"
        android:drawableLeft="@drawable/quantum_ic_search_grey600_24"
        android:drawablePadding="20dp"
        android:padding="20dp"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

